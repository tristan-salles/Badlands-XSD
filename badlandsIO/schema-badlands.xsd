<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:lecode="http://subversion.assembla.com/svn/xsd/XSDlecode"
	targetNamespace="http://subversion.assembla.com/svn/xsd/XSDlecode">
	<xsd:import namespace="http://www.w3.org/2001/XInclude" schemaLocation="http://www.w3.org/2001/XInclude.xsd"/>
	<xsd:import namespace="http://www.w3.org/XML/1998/namespace"
		schemaLocation="http://www.w3.org/2001/03/xml.xsd"/>

	<xsd:annotation>
		<xsd:documentation>This schema documents LECODE's model input file structure for version 3. If you use a
			XML parser, note that we defined a series of attributes 'unit' for each physical property defined in the
			input file, the attribute default value reflects the expected unit of measure for the property ('a','m',
			etc.) </xsd:documentation>
	</xsd:annotation>

	<xsd:element name="lecodeInput">
		<xsd:complexType>
			<xsd:sequence>

				<xsd:element name="struct-mesh" maxOccurs="1" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation> MESH PARAMETERS </xsd:documentation>
					</xsd:annotation>
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Strata_grid">
								<xsd:annotation>
									<xsd:documentation>Stratigraphic mesh parameters definition.</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="gridName" type="xsd:anyURI">
											<xsd:annotation>
												<xsd:documentation>Path to the stratal nodes basement definition file, including the
													file name (.nodes).</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="gridX" type="xsd:integer">
											<xsd:annotation>
												<xsd:documentation>Number of nodes along the Ox axis.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="gridY" type="xsd:integer">
											<xsd:annotation>
												<xsd:documentation>Number of nodes along Oy axis.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="procGrid" type="xsd:integer">
											<xsd:annotation>
												<xsd:documentation>Number of processor used for grid partition.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
                                        <xsd:element name="boundConds" type="xsd:integer" maxOccurs="1" minOccurs="0">
                                            <xsd:annotation>
                                                <xsd:documentation>Sequence of integers defining boundary conditions.
                                                    Border definition order: South - North - West - East
                                                    Conditions value: 0 for reflective (default), 1 for open and 2 for close.
                                                    </xsd:documentation>
                                            </xsd:annotation>
                                        </xsd:element>
										<xsd:element name="noiseDist" type="xsd:double" maxOccurs="1" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Surface is complemented with a noise amplitude value, randomly
													distributed noise in order to stimulate the initial development of drainage
													channels.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
									<xsd:attribute ref="xml:base"/>
								</xsd:complexType>
							</xsd:element>

							<xsd:element minOccurs="0" maxOccurs="1" name="Restart">
								<xsd:annotation>
									<xsd:documentation>Control of the restart functionality. Model can be set to restart from a
										previous simulation step. </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="restartFolder" type="xsd:anyURI">
											<xsd:annotation>
												<xsd:documentation> Name of the previous check_point folder (this file is located in
													the previous output directory within the runfiles directory) </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="restartIt" type="xsd:integer">
											<xsd:annotation>
												<xsd:documentation> Restart iteration number based on the checkpointing files in the
													previous runfile folder. </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
						<xsd:attribute ref="xml:base"/>
					</xsd:complexType>
				</xsd:element>

				<xsd:element name="struct-time" maxOccurs="1" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation> TIME DECLARATION </xsd:documentation>
					</xsd:annotation>

					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Time">
								<xsd:annotation>
									<xsd:documentation>Time control parameters definition. For the Simulation start and end time
										definition, use negative value to represent time before present. For example simulation
										ranging from 21 Ma to 19.6 Ma should have start and end times set to -21000000.0 and
										-19600000.0 respectively.</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="startTime">
											<xsd:annotation>
												<xsd:documentation>Defines the starting time of the experiment
													(years)</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="xsd:double">
														<xsd:attribute name="unit" default="a" use="optional"/>
													</xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="endTime">
											<xsd:annotation>
												<xsd:documentation>Defines the ending time of the experiment, in
													years</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="xsd:double">
														<xsd:attribute name="unit" default="a" use="optional"/>
													</xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="layerTime">
											<xsd:annotation>
												<xsd:documentation>Layer time interval controls how often a new deposit layer is
													created.</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="xsd:double">
														<xsd:attribute name="unit" default="a" use="optional"/>
													</xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="outputTime">
											<xsd:annotation>
												<xsd:documentation>Display time interval. This controls how often results are
													outputed.</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="xsd:double">
														<xsd:attribute name="unit" default="a" use="optional"/>
													</xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="flowInterval">
											<xsd:annotation>
												<xsd:documentation>Sources time sampling interval. This controls how often flow
													walkers are released from their sources. </xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="xsd:double">
														<xsd:attribute name="unit" default="a" use="optional"/>
													</xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="rainInterval">
											<xsd:annotation>
												<xsd:documentation>Rainfall sampling interval. This controls how often rainfall is
													updated. </xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="xsd:double">
														<xsd:attribute name="unit" default="a" use="optional"/>
													</xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
										<xsd:element name="samplingInterval">
											<xsd:annotation>
												<xsd:documentation>Displacement and sea-level time sampling interval. This controls
													how often displacements and sea-level fluctuations are updated. </xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="xsd:double">
														<xsd:attribute name="unit" default="a" use="optional"/>
													</xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>

							<xsd:element maxOccurs="1" minOccurs="0" name="CheckPointing">
								<xsd:annotation>
									<xsd:documentation>Define when checkpointing is done. This is used for restarting the
										simulation from a given time. </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="frequency">
											<xsd:annotation>
												<xsd:documentation> Frequency for checkpointing. This has to be a multiple of the
													outputTime in the Time section. </xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:simpleContent>
													<xsd:extension base="xsd:integer"> </xsd:extension>
												</xsd:simpleContent>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>

						</xsd:sequence>
						<xsd:attribute ref="xml:base"/>
					</xsd:complexType>
				</xsd:element>

				<xsd:element name="struct-ocean" maxOccurs="1" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation> OCEAN PARAMETERS </xsd:documentation>
					</xsd:annotation>

					<xsd:complexType>
						<xsd:sequence>

							<xsd:element maxOccurs="1" minOccurs="0" name="SeaLevelFluctuations">
								<xsd:annotation>
									<xsd:documentation>Sea-level fluctuations definition. </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="seaLevelFile" type="xsd:anyURI">
											<xsd:annotation>
												<xsd:documentation>Name of the sea-level fluctuations file (.sl file), time in years
													and sea-level in metres. </xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>


							<xsd:element maxOccurs="1" minOccurs="0" name="HemipelagicRates">
								<xsd:annotation>
									<xsd:documentation>Sedimentation rates of hemipelagic particles. You will need to define an
										hemipelagic material type with a Material named "Hemi"</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="hemipelagicFile" type="xsd:anyURI">
											<xsd:annotation>
												<xsd:documentation>Name of the hemipelagic sedimentation rate file (.sl file). The
													file is composed of 2 columns one for the contour (in meters negative) and one for
													the rate (m/yr).</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>

						</xsd:sequence>
						<xsd:attribute ref="xml:base"/>
					</xsd:complexType>
				</xsd:element>

				<xsd:element name="struct-geodyn" maxOccurs="1" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation> GEODYNAMIC DECLARATION </xsd:documentation>
					</xsd:annotation>

					<xsd:complexType>

						<xsd:sequence>

							<xsd:element maxOccurs="1" minOccurs="0" name="Displacement">
								<xsd:annotation>
									<xsd:documentation> Definition of the displacement fields within model. </xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="nbDispInterval" type="xsd:integer" default="1">
											<xsd:annotation>
												<xsd:documentation>Defines the number of displacement fields to be imported within the
													simulation.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element minOccurs="1" maxOccurs="unbounded" name="disp">
											<xsd:annotation>
												<xsd:documentation>Individual displacement definition. </xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence minOccurs="1" maxOccurs="1">
													<xsd:element minOccurs="1" maxOccurs="1" name="startDispTime">
														<xsd:annotation>
															<xsd:documentation>Give the start time of the considered displacement interval
																in years.</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:simpleContent>
																<xsd:extension base="xsd:double">
																	<xsd:attribute name="unit" default="a" use="optional"> </xsd:attribute>
																</xsd:extension>
															</xsd:simpleContent>
														</xsd:complexType>
													</xsd:element>
													<xsd:element minOccurs="1" maxOccurs="1" name="endDispTime">
														<xsd:annotation>
															<xsd:documentation>Give the end time of the considered displacement interval in
																years.</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:simpleContent>
																<xsd:extension base="xsd:double">
																	<xsd:attribute name="unit" default="a" use="optional"> </xsd:attribute>
																</xsd:extension>
															</xsd:simpleContent>
														</xsd:complexType>
													</xsd:element>
													<xsd:element minOccurs="1" maxOccurs="1" name="dispFile" type="xsd:string">
														<xsd:annotation>
															<xsd:documentation>Give the displacement file name for the considered time
																interval in m.</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>

							<xsd:element maxOccurs="1" minOccurs="0" name="Flexure">
								<xsd:annotation>
									<xsd:documentation>Modelling parameter for flexural isostatic
										deformations.</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="flexuralInterval" type="xsd:double">
											<xsd:annotation>
												<xsd:documentation>Flexure update time interval (in years). The value should be a
													multiple of the sampling interval defined above.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="flexuralRigidity" type="xsd:double">
											<xsd:annotation>
												<xsd:documentation>Plate flexural rigidity in Nm.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
										<xsd:element name="crustDensity" type="xsd:double">
											<xsd:annotation>
												<xsd:documentation>Density of the crust in kg/m3.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>

						</xsd:sequence>

						<xsd:attribute ref="xml:base"/>
					</xsd:complexType>
				</xsd:element>

				<xsd:element name="struct-sediments" maxOccurs="1" minOccurs="1">
					<xsd:annotation>
						<xsd:documentation> SEDIMENTS DEFINITION </xsd:documentation>
					</xsd:annotation>


					<xsd:complexType>
						<xsd:sequence>
							<xsd:element minOccurs="1" maxOccurs="1" name="Materials">
								<xsd:annotation>
									<xsd:documentation>Material definition parameters. Indicative material size: pebble 4-64 mm,
										granule 2-4 mm, vcse 1-2 mm, cse 0.5-1 mm, med 0.25-0.5 mm, fn 0.125-0.25, vfn 0.062-0.125
										mm, slt 0.0039-0.062 mm, clay lower than 0.0039 mm. The definition of the material classes
										starts with the declaration of the siliciclastics, then the carbonates and finally the
										organics. For each 3 types of materials you need to organise them by decreasing diametre
										size.</xsd:documentation>
								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence minOccurs="1" maxOccurs="1">
										<xsd:sequence>
											<xsd:element name="nbSiliciclastics" type="xsd:integer">
												<xsd:annotation>
													<xsd:documentation>Total number of siliciclastics</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
											<!--xsd:element name="nbCarbonates" type="xsd:integer">
                                                <xsd:annotation>
                                                  <xsd:documentation>Total number of
                                                  carbonates</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element>
                                            <xsd:element name="nbOrganics" type="xsd:integer">
                                                <xsd:annotation>
                                                  <xsd:documentation>Total number of
                                                  organics</xsd:documentation>
                                                </xsd:annotation>
                                            </xsd:element-->
											<xsd:element name="minSlope" type="xsd:double" default="0.0001">
												<xsd:annotation>
													<xsd:documentation>Minimum slope of deposited sediment taken into account for slope
														diffusion calculation. Below this threshold value, slope diffusion is not
														computed.</xsd:documentation>
												</xsd:annotation>
											</xsd:element>
										</xsd:sequence>
										<xsd:element minOccurs="0" maxOccurs="unbounded" name="si">
											<xsd:annotation>
												<xsd:documentation>Parameters collection for the considered siliciclastic
													material.</xsd:documentation>
											</xsd:annotation>
											<xsd:complexType>
												<xsd:sequence minOccurs="1" maxOccurs="1">
													<xsd:element minOccurs="1" maxOccurs="1" name="materialName" type="xsd:string">
														<xsd:annotation>
															<xsd:documentation>Unique name of the considered material</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element minOccurs="1" maxOccurs="1" name="diameter">
														<xsd:annotation>
															<xsd:documentation>grain size diameter of the considered material
																(mm)</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:simpleContent>
																<xsd:extension base="xsd:double">
																	<xsd:attribute name="unit" default="mm" use="optional"/>
																</xsd:extension>
															</xsd:simpleContent>
														</xsd:complexType>
													</xsd:element>
													<xsd:element minOccurs="1" maxOccurs="1" name="density">
														<xsd:annotation>
															<xsd:documentation>Density of the considered material
																(kg/m3)</xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:simpleContent>
																<xsd:extension base="xsd:double">
																	<xsd:attribute name="unit" default="kg/m3" use="optional"/>
																</xsd:extension>
															</xsd:simpleContent>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="slopeMarine" type="xsd:double">
														<xsd:annotation>
															<xsd:documentation> Sediment diffusion slope in marine environment (dz/dx).
															</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="creepMarine" type="xsd:double">
														<xsd:annotation>
															<xsd:documentation> Sediment creep diffusion coefficient in marine environment
																(m2/yr).</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="slopeAerial" type="xsd:double">
														<xsd:annotation>
															<xsd:documentation> Sediment diffusion slope in aerial environment (dz/dx).
															</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="creepAerial" type="xsd:double">
														<xsd:annotation>
															<xsd:documentation> Sediment creep diffusion coefficient in aerial environment
																(m2/yr).</xsd:documentation>
														</xsd:annotation>
													</xsd:element>
													<xsd:element name="erodibility">
														<xsd:annotation>
															<xsd:documentation> Intrinsic bedrock erosion coefficient (see Clevis et al.
																2003 for values range) </xsd:documentation>
														</xsd:annotation>
														<xsd:complexType>
															<xsd:simpleContent>
																<xsd:extension base="xsd:double">
																	<xsd:attribute name="unit" default="a^(-2/3)" use="optional"/>
																</xsd:extension>
															</xsd:simpleContent>
														</xsd:complexType>
													</xsd:element>
													<xsd:element name="transportEff" type="xsd:double">
														<xsd:annotation>
															<xsd:documentation>Sediment transport efficiency for considered material. It
																corresponds to the fluvial transport efficiency coefficient which values range
																between 0.001 and 1.0. When applying the landscape evolution transport mode
																(option 3) it should be around 0.01. For the flow walker mode (option 0) it
																will vary depending on the type of sediment but is usually set to a higher
																value about 10 times greater. </xsd:documentation>
														</xsd:annotation>
													</xsd:element>
												</xsd:sequence>
											</xsd:complexType>
										</xsd:element>
										<!--xsd:element minOccurs="0" maxOccurs="unbounded" name="carb">
                                            <xsd:annotation>
                                                <xsd:documentation>Parameters collection for the
                                                  considered carbonate material.</xsd:documentation>
                                            </xsd:annotation>
                                            <xsd:complexType>
                                                <xsd:sequence minOccurs="1" maxOccurs="1">
                                                  <xsd:element minOccurs="1" maxOccurs="1"
                                                  name="MaterialName" type="xsd:string">
